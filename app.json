{
  "expo": {
    "name": "NativeTuto",
    "slug": "NativeTuto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "height":10, 
      "width":270,
      "backgroundColor": "#26282C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#26282C"
      }
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    }
  }
}
